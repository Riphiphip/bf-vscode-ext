{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Brainfuck",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.brainfuck",
				"match": "\\[|\\]"
			},
			{
				"name":"keyword.operator.brainfuck",
				"match": "[+-]"
			},
			{
				"name":"variable.shift.brainfuck",
				"match": "[<>]"
			},
			{
				"name":"keyword.other.io.brainfuck",
				"match": "[\\.,]"
			},
			{
				"name": "comment.character.brainfuck",
				"match": "[^+-<>\\.,\\[\\]]"
			}
		]
		}
	},
	"scopeName": "source.brainfuck"
}